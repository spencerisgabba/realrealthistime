{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "pip": "requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/__handler__.py"
    },
    {
      "src": "/(.*)",
      "dest": "/project/index.html"
    }
  ],
  "functions": {
    "api/post_list.py": {
      "runtime": "vercel-python@0.24.0",
      "maxDuration": 10
    },
    "api/post_detail.py": {
      "runtime": "vercel-python@0.24.0",
      "maxDuration": 10
    },
    "api/post_by_tag.py": {
      "runtime": "vercel-python@0.24.0",
      "maxDuration": 10
    }
  }
}
